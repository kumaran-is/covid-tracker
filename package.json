{
  "name": "covid-tracker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod-build": "ng build --prod --base-href=./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "aws-deploy": "npm run prod-build && aws s3 sync dist/covid-tracker/ s3://kumaranis.com --delete",
    "aws-ls": "aws s3 ls s3://kumaranis.com/ --recursive --human-readable --summarize",
    "aws-remove": "aws s3 rb s3://kumaranis.com --force",
    "aws-remove-www": "aws s3 rb s3://www.kumaranis.com --force",
    "aws-cb": "aws s3 mb s3://kumaranis.com --region us-east-1",
    "aws-cb-www": "aws s3 mb s3://www.kumaranis.com --region us-east-1",
    "aws-website": "aws s3 website s3://kumaranis.com/ --index-document index.html --error-document index.html",
    "aws-b-policy": "aws s3api put-bucket-policy --bucket kumaranis.com --policy file://docs/bucketpolicy.json",
    "aws-b-policy-www": "aws s3api put-bucket-policy --bucket www.kumaranis.com --policy file://docs/bucketpolicy-www.json",
    "git-publish": "npm run prod-build && npx angular-cli-ghpages --dir=dist/covid-tracker --branch=gh-pages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "ag-grid-angular": "~24.1.0",
    "ag-grid-community": "~24.1.0",
    "rxjs": "~6.6.0",
    "tslib": "~2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/node": "~12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "angular-cli-ghpages": "~0.6.2",
    "codelyzer": "~6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
